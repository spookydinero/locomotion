{
  "name": "locomotion-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "node scripts/run-migrations.js",
    "db:seed": "node scripts/seed-test-data.js",
    "db:create-users": "node scripts/create-test-users.js",
    "db:verify": "node scripts/verify-setup.js",
    "db:setup-users": "node scripts/setup-users.js",
    "db:reset": "node scripts/reset-database.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --config tests/config/jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config tests/config/jest.config.js --testPathPattern=integration",
    "test:e2e": "cypress run --config-file tests/config/cypress.config.js",
    "test:security": "jest --config tests/config/jest.config.js --testPathPattern=security",
    "test:performance": "jest --config tests/config/jest.config.js --testPathPattern=performance",
    "test:setup": "npm run db:migrate && npm run db:seed",
    "test:report": "jest --config tests/config/jest.config.js --coverage --coverageReporters=html",
    "test:coverage": "jest --config tests/config/jest.config.js --coverage",
    "auth:clear": "node scripts/clear-auth-sessions.js",
    "qr:regenerate": "node scripts/regenerate-qr-codes.js",
    "qr:validate": "node scripts/validate-qr-codes.js"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "dotenv": "^17.2.2",
    "next": "15.5.4",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
